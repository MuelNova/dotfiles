#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='ls --color=auto'
alias grep='grep --color=auto'
PS1='[\u@\h \W]\$ '

[[ -s "/home/nova/.gvm/scripts/gvm" ]] && source "/home/nova/.gvm/scripts/gvm"

[ -s "/home/nova/.jabba/jabba.sh" ] && source "/home/nova/.jabba/jabba.sh"
